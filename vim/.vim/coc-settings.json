{
    "coc.preferences.formatOnSave": true,
    "suggest.filterGraceful": true,
    "yank.highlight.enable": true,
    "yank.highlight.duration": 150,
    "yank.list.maxsize": 300,
    "yank.enableCompletion": true,
    "yank.priority": 90,
    "yank.limit": 3,
    "explorer.icon.enableNerdfont": true,
    "explorer.file.showHiddenFiles": true,
    "explorer.file.column.indent.indentLine": true,
    "explorer.git.enable": true,
    "explorer.git.showIgnored": true,
    "explorer.keyMappings.global": {
        "o": [
            "wait",
            "expanded?",
            "collapse",
            [
                "expandable?",
                "expand",
                "open"
            ]
        ],
        "E": "open:split",
        "s": "open:vsplit",
        "d": "delete"
    },
    "diagnostic.errorSign": " ",
    "diagnostic.warningSign": " ",
    "diagnostic.hintSign": " ",
    "diagnostic.infoSign": " ",
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualTextPrefix": " ❯❯❯ ",
    "diagnostic.virtualText": false,
    "clangd.path": "clangd",
    "snippets.ultisnips.pythonPrompt": false
}
