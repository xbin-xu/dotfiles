{
    //--------------------------------------------------------------------------
    // -- Geneal {
    "oneDarkPro.italic": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.activityBar.location": "hidden",
    "window.commandCenter": true,
    "security.workspace.trust.untrustedFiles": "open",
    "files.exclude": {
        "**/*.exe": true
    },
    "files.autoGuessEncoding": true,
    "files.autoSave": "onFocusChange",
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80
    ],
    "editor.mouseWheelZoom": true,
    "editor.minimap.enabled": false,
    "editor.stickyScroll.enabled": true,
    "editor.cursorSurroundingLines": 5,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.fontFamily": "'JetBrainsMono Nerd Font', Consolas",
    "editor.fontLigatures": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[c++]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "black-formatter.args": [
        "--skip-string-normalization"
    ],
    "diffEditor.renderSideBySide": false,
    "explorer.compactFolders": false,
    "terminal.integrated.profiles.windows": {
        "Git Bash": {
            "source": "Git Bash",
            "path": [
                "D:\\Software\\Scoop\\apps\\git\\current\\bin\\bash.exe"
            ],
            "icon": "terminal-bash"
        }
    },
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen"
    ],
    // }
    //--------------------------------------------------------------------------
    // -- Markdown {
    "[markdown]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        }
    },
    "markdown-preview-enhanced.previewTheme": "atom-dark.css",
    "markdown-preview-enhanced.codeBlockTheme": "one-dark.css",
    "MarkdownPaste.path": "${workspaceFolder}/imgs",
    "MarkdownPaste.namePrefix": "img-",
    // }
    //--------------------------------------------------------------------------
    // -- Project Manager {
    "projectManager.git.baseFolders": [
        "d:/Workbench/"
    ],
    "projectManager.sortList": "Recent",
    // }
    //--------------------------------------------------------------------------
    // -- neovim {
    // "vscode-neovim.useWSL": true,
    "editor.lineNumbers": "relative",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    // }
