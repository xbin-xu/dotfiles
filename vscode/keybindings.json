// 将键绑定放在此文件中以覆盖默认值
[
    // -- which key
    //--------------------------------------------------------------------------
    {
        "key": "space",
        "command": "whichkey.show",
        "when": "!inputFocus && (editorTextFocus || sideBarFocus)"
    },
    {
        "key": "backspace",
        "command": "whichkey.undoKey",
        "when": "whichkeyVisible"
    },
    {
        "key": "e",
        "command": "whichkey.triggerKey",
        "args": {
            "key": "e",
            "when": "sideBarVisible"
        },
        "when": "whichkeyVisible && sideBarVisible"
    },
    {
        "key": "p",
        "command": "whichkey.triggerKey",
        "args": {
            "key": "p",
            "when": "activeEditorIsPinned"
        },
        "when": "whichkeyVisible && activeEditorIsPinned"
    },
    // -- expand regin
    //--------------------------------------------------------------------------
    {
        "key": "v",
        "command": "expand_region",
        "when": "editorTextFocus && ((vim.mode == 'Visual' || vim.mode == 'VisualLine' || vim.mode == 'VisualBlock') || (neovim.init && neovim.mode == 'visual'))"
    },
    {
        "key": "ctrl+=",
        "command": "expand_region",
        "when": "editorTextFocus"
    },
    {
        "key": "backspace",
        "command": "undo_expand_region",
        "when": "editorTextFocus && editorHasSelection"
    },
    {
        "key": "ctrl+w",
        "command": "-expand_region",
        "when": "editorTextFocus"
    },
    // -- navigation
    //--------------------------------------------------------------------------
    // ---- list focus
    {
        "key": "h",
        "command": "list.collapse",
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    {
        "key": "j",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    {
        "key": "k",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    {
        "key": "l",
        "command": "list.expand",
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    {
        "key": "o",
        "command": "list.select",
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    {
        "key": "s",
        "command": "runCommands",
        "args": {
            "commands": [
                "list.select",
                "workbench.action.moveEditorToRightGroup"
            ]
        },
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    {
        "key": "v",
        "command": "runCommands",
        "args": {
            "commands": [
                "list.select",
                "workbench.action.moveEditorToBelowGroup"
            ]
        },
        "when": "listFocus && !inputFocus || panelFocus && activePanel == 'workbench.panel.markers'"
    },
    // ---- windows
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateDown"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateRight",
        "when": "!terminalFocus"
    },
    // ---- quick open
    {
        "key": "ctrl+j",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    },
    // ---- intelliSense suggestions
    {
        "key": "ctrl+j",
        "command": "selectNextSuggestion",
        "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
    },
    {
        "key": "ctrl+k",
        "command": "selectPrevSuggestion",
        "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
    },
    {
        "key": "tab",
        "command": "selectNextSuggestion",
        "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
    },
    {
        "key": "shift+tab",
        "command": "selectPrevSuggestion",
        "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
    },
    // ---- code action
    {
        "key": "ctrl+j",
        "command": "selectNextCodeAction",
        "when": "codeActionMenuVisible"
    },
    {
        "key": "ctrl+k",
        "command": "selectPrevCodeAction",
        "when": "codeActionMenuVisible"
    },
    // ---- search
    {
        "key": "ctrl+j",
        "command": "search.action.focusSearchList",
        "when": "searchViewletFocus"
    },
    {
        "key": "ctrl+k",
        "command": "search.action.focusSearchFromResults",
        "when": "searchViewletFocus"
    },
    // -- search history
    //--------------------------------------------------------------------------
    {
        "key": "ctrl+n",
        "command": "history.showNext",
        "when": "historyNavigationForwardsEnabled && historyNavigationWidgetFocus && !isComposing && !suggestWidgetVisible"
    },
    {
        "key": "ctrl+p",
        "command": "history.showPrevious",
        "when": "historyNavigationBackwardsEnabled && historyNavigationWidgetFocus && !isComposing && !suggestWidgetVisible"
    },
    // -- file explorer
    //--------------------------------------------------------------------------
    {
        "key": "a",
        "command": "explorer.newFile",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "shift+a",
        "command": "explorer.newFolder",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "d",
        "command": "deleteFile",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "r",
        "command": "renameFile",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "y",
        "command": "filesExplorer.copy",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "shift+y",
        "command": "copyFilePath",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "p",
        "command": "filesExplorer.paste",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+e",
        "command": "revealFileInOS",
        "when": "!editorFocus"
    },
    // -- terminal
    //--------------------------------------------------------------------------
    {
        "key": "ctrl+/",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+alt+m",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+alt+shift+k",
        "command": "workbench.action.terminal.resizePaneUp"
    },
    {
        "key": "ctrl+alt+shift+j",
        "command": "workbench.action.terminal.resizePaneDown"
    },
    {
        "key": "ctrl+alt+shift+l",
        "command": "workbench.action.terminal.resizePaneRight"
    },
    {
        "key": "ctrl+alt+shift+h",
        "command": "workbench.action.terminal.resizePaneLeft"
    },
    {
        "key": "ctrl+alt+\\",
        "command": "workbench.action.terminal.split",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+alt+c",
        "command": "workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+alt+q",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+alt+,",
        "command": "workbench.action.terminal.rename",
        "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+alt+h",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+alt+l",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+alt+p",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "ctrl+alt+n",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    // -- move lines
    //--------------------------------------------------------------------------
    {
        "key": "alt+j",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+k",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
]